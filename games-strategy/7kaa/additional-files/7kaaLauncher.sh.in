#!/bin/bash
yoshi $(finddir B_SYSTEM_DATA_DIRECTORY)/7kaa/yoshiLauncher>/tmp/7kaaYoshout
while IFS='' read -r line || [[ -n "$line" ]]; do
	var1=$(echo $line | cut -f1 -d=)
	var2=$(echo $line | cut -f2 -d=)
	eval $var1='$var2'
done < "/tmp/7kaaYoshout"
rm /tmp/7kaaYoshout

if [ $cancel -eq 1 ]
then
 exit 0
else
 echo $res
	case $res in
		option1)
			width=640
			height=480
			;;
                option2)
			width=800
			height=600
			;;
		option3)
			width=1024
			height=768
			;;
		option4)
			width=1280
			height=1024
			;;
		option5)
			width=1400
			height=1050
			;;
		option6)
			width=1600
			height=1200
			;;
		*)
			width=640
			height=480
	esac
	
	if [ "$fs" == "ON" ]
	then
		fs=true
	else
		fs=false
	fi

	if [ "$desktopFs" == "ON" ]
	then
		desktopFs=true
	else
		$desktopFs=false
	fi
	echo -e "vga_full_screen="$fs"\nvga_full_screen_desktop="$desktopFs"\nvga_window_width="$width"\nvga_window_height="$height > $(finddir B_USER_SETTINGS_DIRECTORY)/7kfans.com/7kaa/config.txt
	$(finddir B_SYSTEM_BIN_DIRECTORY)/7kaa@archSuffix@ $@
fi
